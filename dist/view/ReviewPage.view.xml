        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content>
                    <ToolbarSpacer width=""/>
                    <Button text = "Datayı Yükle" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" press="_onEditButtonPress3"></Button>
                    <Button text="Düzenle" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" press="_onEditButtonPress"/>
                    <Button text="Kaydet" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onEditButtonPress4"/>
                    <Button text="İptal" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onEditButtonPress2"/>
                </content>
            </OverflowToolbar>
        </footer>
        <headerContent/>
        <subHeader/>
        <customHeader/>

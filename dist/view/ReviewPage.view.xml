                                    <Input id="gozetmen" type="Text"  showValueHelp="false"  enabled="false" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="5000"/>                                </form:fields>
                                
                            </form:FormElement>
                            <form:FormElement visible="true">
                                <form:label>
                                    <Label text="Konşimento Türü" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>

                            <Input id="sonkullanici" type="Text"  showValueHelp="false"  enabled="false" visible="true" width="auto" valueHelpOnly="false" required="false" valueStateText="Invalid entry" maxLength="5000"/>                               


                            <layout:HorizontalLayout>
                                <layout:content/>
                            </layout:HorizontalLayout>
                            <layout:HorizontalLayout>
                                <layout:content/>
                            </layout:HorizontalLayout>
                            <layout:HorizontalLayout>
                                <layout:content/>
                            </layout:HorizontalLayout>
                            <layout:HorizontalLayout>
                                <layout:content/>
                            </layout:HorizontalLayout>
                            <layout:HorizontalLayout>
                                <layout:content/>
                            </layout:HorizontalLayout>
